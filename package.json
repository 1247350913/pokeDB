{
  "name": "pokedb",
  "private": true,
  "packageManager": "pnpm@10.18.3",
  "scripts": {
    "build": "turbo run build",
    
    "typecheck": "turbo run typecheck",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rimraf node_modules .turbo",
    
    "dev-desktop": "turbo run dev --parallel --filter=@apps/desktop-renderer --filter=@apps/desktop-electron"
  },
  "devDependencies": {
    "turbo": "^2.5.8",
    "typescript": "^5.6.3",
    "rimraf": "^6.0.1",
    "@types/node": "24.6.0",
    "eslint": "9.36.0",
    "@eslint/js": "9.36.0",
    "typescript-eslint": "8.45.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.22",
    "globals": "16.4.0"
  }
}
